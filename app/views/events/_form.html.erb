<%= form_for @event do |f| %>
  <%= f.label :title%><br/>
  <%= f.text_field :title%><br />
  
  <%= f.label :description%><br/>
  <%= f.text_area :description%><br />
  
  <%= f.label "Event Schedule"%><br/>
  <%= f.text_field :event_at%><br />
  
  <%= f.label :location%><br/>
  <%= f.text_field :location%><br />
  
  <%= f.submit "Submit" %>
  
  
<% end %> 
